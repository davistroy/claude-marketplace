<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  id="Definitions_1"
                  targetNamespace="http://example.org/bpmn">
  <bpmn:process id="Process_1" name="Decision Process" isExecutable="true">
    <bpmn:startEvent id="Start_1" name="Start"/>
    <bpmn:exclusiveGateway id="Gateway_1" name="Decision?" default="Flow_Default"/>
    <bpmn:task id="Task_Yes" name="Yes Path"/>
    <bpmn:task id="Task_No" name="No Path"/>
    <bpmn:exclusiveGateway id="Gateway_2" name="Merge"/>
    <bpmn:endEvent id="End_1" name="End"/>

    <bpmn:sequenceFlow id="Flow_1" sourceRef="Start_1" targetRef="Gateway_1"/>
    <bpmn:sequenceFlow id="Flow_Yes" name="Yes" sourceRef="Gateway_1" targetRef="Task_Yes">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">condition == true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Default" sourceRef="Gateway_1" targetRef="Task_No"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_Yes" targetRef="Gateway_2"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_No" targetRef="Gateway_2"/>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Gateway_2" targetRef="End_1"/>
  </bpmn:process>
</bpmn:definitions>
