    <bpmn:sequenceFlow id="Flow_Invalid_1" sourceRef="NonExistent_Source" targetRef="Task_1"/>
    <bpmn:sequenceFlow id="Flow_Invalid_2" sourceRef="Task_1" targetRef="NonExistent_Target"/>
