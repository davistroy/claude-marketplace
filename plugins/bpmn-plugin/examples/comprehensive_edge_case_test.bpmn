<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
    xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
    xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
    xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
    xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    id="Definitions_EdgeCaseTest"
    name="Comprehensive Edge Case Test"
    targetNamespace="http://test.example.com/bpmn/edge-cases"
    exporter="Claude AI"
    exporterVersion="1.0">

    <!-- ============================================================== -->
    <!-- MESSAGE DEFINITIONS -->
    <!-- ============================================================== -->
    <bpmn:message id="Message_Request" name="Request"/>
    <bpmn:message id="Message_Response" name="Response"/>
    <bpmn:message id="Message_Notification" name="Notification"/>
    <bpmn:message id="Message_Confirmation" name="Confirmation"/>
    <bpmn:message id="Message_Escalation" name="Escalation"/>

    <!-- ============================================================== -->
    <!-- SIGNAL DEFINITIONS -->
    <!-- ============================================================== -->
    <bpmn:signal id="Signal_Alert" name="SystemAlert"/>
    <bpmn:signal id="Signal_Complete" name="ProcessComplete"/>
    <bpmn:signal id="Signal_Cancel" name="CancelOperation"/>

    <!-- ============================================================== -->
    <!-- ERROR DEFINITIONS -->
    <!-- ============================================================== -->
    <bpmn:error id="Error_Validation" name="ValidationError" errorCode="ERR_VALIDATION"/>
    <bpmn:error id="Error_Timeout" name="TimeoutError" errorCode="ERR_TIMEOUT"/>
    <bpmn:error id="Error_System" name="SystemError" errorCode="ERR_SYSTEM"/>

    <!-- ============================================================== -->
    <!-- ESCALATION DEFINITIONS -->
    <!-- ============================================================== -->
    <bpmn:escalation id="Escalation_Warning" name="Warning" escalationCode="ESC_WARN"/>
    <bpmn:escalation id="Escalation_Critical" name="Critical" escalationCode="ESC_CRIT"/>

    <!-- ============================================================== -->
    <!-- DATA STORE DEFINITIONS -->
    <!-- ============================================================== -->
    <bpmn:dataStore id="DataStore_MainDB" name="Main Database"/>
    <bpmn:dataStore id="DataStore_Cache" name="Redis Cache"/>
    <bpmn:dataStore id="DataStore_Archive" name="Archive Storage"/>

    <!-- ============================================================== -->
    <!-- COLLABORATION: Multi-Pool Process -->
    <!-- ============================================================== -->
    <bpmn:collaboration id="Collaboration_EdgeCaseTest">

        <!-- Pool 1: Customer (External, no process ref - BLACK BOX) -->
        <bpmn:participant id="Pool_Customer" name="External Customer">
            <!-- This pool has NO processRef - it's a black box -->
        </bpmn:participant>

        <!-- Pool 2: Main System with Lanes -->
        <bpmn:participant id="Pool_MainSystem" name="Main Processing System" processRef="Process_Main"/>

        <!-- Pool 3: Secondary System with Lanes -->
        <bpmn:participant id="Pool_Secondary" name="Secondary Services" processRef="Process_Secondary"/>

        <!-- Pool 4: External API (no lanes, single partition) -->
        <bpmn:participant id="Pool_ExternalAPI" name="External API Gateway" processRef="Process_ExternalAPI"/>

        <!-- Pool 5: Audit System -->
        <bpmn:participant id="Pool_Audit" name="Audit and Compliance" processRef="Process_Audit"/>

        <!-- Pool 6: Legacy System (minimal) -->
        <bpmn:participant id="Pool_Legacy" name="Legacy Backend" processRef="Process_Legacy"/>

        <!-- Message Flows (Cross-Pool) -->
        <bpmn:messageFlow id="MsgFlow_1" name="Initial Request" sourceRef="Pool_Customer" targetRef="Start_Message_Main" messageRef="Message_Request"/>
        <bpmn:messageFlow id="MsgFlow_2" name="Response to Customer" sourceRef="Task_SendResponse" targetRef="Pool_Customer" messageRef="Message_Response"/>
        <bpmn:messageFlow id="MsgFlow_3" name="API Call" sourceRef="Task_CallExternalAPI" targetRef="Start_API" messageRef="Message_Request"/>
        <bpmn:messageFlow id="MsgFlow_4" name="API Response" sourceRef="Task_APIRespond" targetRef="Event_CatchAPIResponse" messageRef="Message_Response"/>
        <bpmn:messageFlow id="MsgFlow_5" name="Audit Notification" sourceRef="Task_LogAudit" targetRef="Start_Audit" messageRef="Message_Notification"/>
        <bpmn:messageFlow id="MsgFlow_6" name="Legacy Sync" sourceRef="Task_SyncData" targetRef="Pool_Legacy" messageRef="Message_Request"/>

    </bpmn:collaboration>

    <!-- ============================================================== -->
    <!-- PROCESS 1: Main Processing System (with lanes) -->
    <!-- ============================================================== -->
    <bpmn:process id="Process_Main" name="Main Processing" isExecutable="true">

        <bpmn:laneSet id="LaneSet_Main">
            <!-- Lane 1: Reception -->
            <bpmn:lane id="Lane_Reception" name="Reception Layer">
                <bpmn:flowNodeRef>Start_Message_Main</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Start_Timer_Scheduled</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Start_Signal_Alert</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Task_ReceiveRequest</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Gateway_RequestType</bpmn:flowNodeRef>
            </bpmn:lane>

            <!-- Lane 2: Validation -->
            <bpmn:lane id="Lane_Validation" name="Validation Layer">
                <bpmn:flowNodeRef>Task_ValidateInput</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Gateway_ValidationResult</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Task_RejectInvalid</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>End_Rejected</bpmn:flowNodeRef>
            </bpmn:lane>

            <!-- Lane 3: Processing -->
            <bpmn:lane id="Lane_Processing" name="Core Processing">
                <bpmn:flowNodeRef>SubProcess_MainProcessing</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Gateway_ProcessingOutcome</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Event_CatchAPIResponse</bpmn:flowNodeRef>
            </bpmn:lane>

            <!-- Lane 4: Business Rules -->
            <bpmn:lane id="Lane_BusinessRules" name="Business Rules Engine">
                <bpmn:flowNodeRef>Task_ApplyBusinessRules</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Gateway_RulesDecision</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Task_HandleException</bpmn:flowNodeRef>
            </bpmn:lane>

            <!-- Lane 5: Response -->
            <bpmn:lane id="Lane_Response" name="Response Handler">
                <bpmn:flowNodeRef>Task_PrepareResponse</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Task_SendResponse</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>End_Success</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>End_Error</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>End_Terminate</bpmn:flowNodeRef>
            </bpmn:lane>

            <!-- Lane 6: Integration -->
            <bpmn:lane id="Lane_Integration" name="External Integration">
                <bpmn:flowNodeRef>Task_CallExternalAPI</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Task_SyncData</bpmn:flowNodeRef>
                <bpmn:flowNodeRef>Task_LogAudit</bpmn:flowNodeRef>
            </bpmn:lane>
        </bpmn:laneSet>

        <!-- ============== START EVENTS ============== -->

        <!-- Message Start -->
        <bpmn:startEvent id="Start_Message_Main" name="Message Received">
            <bpmn:outgoing>Flow_1</bpmn:outgoing>
            <bpmn:messageEventDefinition id="MsgEvtDef_Start" messageRef="Message_Request"/>
        </bpmn:startEvent>

        <!-- Timer Start -->
        <bpmn:startEvent id="Start_Timer_Scheduled" name="Scheduled Trigger">
            <bpmn:outgoing>Flow_TimerToReceive</bpmn:outgoing>
            <bpmn:timerEventDefinition id="TimerEvtDef_Start">
                <bpmn:timeCycle>0 0 * * * ?</bpmn:timeCycle>
            </bpmn:timerEventDefinition>
        </bpmn:startEvent>

        <!-- Signal Start -->
        <bpmn:startEvent id="Start_Signal_Alert" name="Alert Signal">
            <bpmn:outgoing>Flow_SignalToReceive</bpmn:outgoing>
            <bpmn:signalEventDefinition id="SignalEvtDef_Start" signalRef="Signal_Alert"/>
        </bpmn:startEvent>

        <!-- ============== RECEPTION TASKS ============== -->

        <bpmn:receiveTask id="Task_ReceiveRequest" name="Receive and Parse Request">
            <bpmn:incoming>Flow_1</bpmn:incoming>
            <bpmn:incoming>Flow_TimerToReceive</bpmn:incoming>
            <bpmn:incoming>Flow_SignalToReceive</bpmn:incoming>
            <bpmn:outgoing>Flow_2</bpmn:outgoing>
        </bpmn:receiveTask>

        <!-- XOR Gateway with 4+ outgoing flows -->
        <bpmn:exclusiveGateway id="Gateway_RequestType" name="Request Type?" default="Flow_Default">
            <bpmn:incoming>Flow_2</bpmn:incoming>
            <bpmn:outgoing>Flow_TypeA</bpmn:outgoing>
            <bpmn:outgoing>Flow_TypeB</bpmn:outgoing>
            <bpmn:outgoing>Flow_TypeC</bpmn:outgoing>
            <bpmn:outgoing>Flow_Default</bpmn:outgoing>
        </bpmn:exclusiveGateway>

        <!-- ============== VALIDATION TASKS ============== -->

        <bpmn:userTask id="Task_ValidateInput" name="Validate Input Data">
            <bpmn:incoming>Flow_TypeA</bpmn:incoming>
            <bpmn:incoming>Flow_TypeB</bpmn:incoming>
            <bpmn:incoming>Flow_TypeC</bpmn:incoming>
            <bpmn:incoming>Flow_Default</bpmn:incoming>
            <bpmn:outgoing>Flow_3</bpmn:outgoing>
        </bpmn:userTask>

        <!-- Inclusive Gateway (OR) with 3+ outgoing -->
        <bpmn:inclusiveGateway id="Gateway_ValidationResult" name="Validation Result?">
            <bpmn:incoming>Flow_3</bpmn:incoming>
            <bpmn:outgoing>Flow_Valid</bpmn:outgoing>
            <bpmn:outgoing>Flow_PartialValid</bpmn:outgoing>
            <bpmn:outgoing>Flow_Invalid</bpmn:outgoing>
        </bpmn:inclusiveGateway>

        <bpmn:serviceTask id="Task_RejectInvalid" name="Reject Invalid Request">
            <bpmn:incoming>Flow_Invalid</bpmn:incoming>
            <bpmn:outgoing>Flow_ToRejected</bpmn:outgoing>
        </bpmn:serviceTask>

        <bpmn:endEvent id="End_Rejected" name="Request Rejected">
            <bpmn:incoming>Flow_ToRejected</bpmn:incoming>
            <bpmn:errorEventDefinition id="ErrEvtDef_Rejected" errorRef="Error_Validation"/>
        </bpmn:endEvent>

        <!-- ============== MAIN PROCESSING SUBPROCESS ============== -->

        <bpmn:subProcess id="SubProcess_MainProcessing" name="Main Processing Workflow">
            <bpmn:incoming>Flow_Valid</bpmn:incoming>
            <bpmn:incoming>Flow_PartialValid</bpmn:incoming>
            <bpmn:outgoing>Flow_ProcessingComplete</bpmn:outgoing>

            <!-- Subprocess Start -->
            <bpmn:startEvent id="SubStart_Main" name="Begin Processing"/>

            <!-- Parallel Gateway (AND) with 3+ outgoing -->
            <bpmn:parallelGateway id="Gateway_Parallel_Split" name="Execute in Parallel">
                <bpmn:incoming>SubFlow_1</bpmn:incoming>
                <bpmn:outgoing>SubFlow_P1</bpmn:outgoing>
                <bpmn:outgoing>SubFlow_P2</bpmn:outgoing>
                <bpmn:outgoing>SubFlow_P3</bpmn:outgoing>
            </bpmn:parallelGateway>

            <!-- Parallel Branch 1: Script Task -->
            <bpmn:scriptTask id="Task_Script_Transform" name="Transform Data" scriptFormat="javascript">
                <bpmn:incoming>SubFlow_P1</bpmn:incoming>
                <bpmn:outgoing>SubFlow_P1_End</bpmn:outgoing>
                <bpmn:script>return data.transform();</bpmn:script>
            </bpmn:scriptTask>

            <!-- Parallel Branch 2: Business Rule Task -->
            <bpmn:businessRuleTask id="Task_BusinessRule" name="Apply Decision Table">
                <bpmn:incoming>SubFlow_P2</bpmn:incoming>
                <bpmn:outgoing>SubFlow_P2_End</bpmn:outgoing>
            </bpmn:businessRuleTask>

            <!-- Parallel Branch 3: Manual Task -->
            <bpmn:manualTask id="Task_ManualReview" name="Manual Review Required">
                <bpmn:incoming>SubFlow_P3</bpmn:incoming>
                <bpmn:outgoing>SubFlow_P3_End</bpmn:outgoing>
            </bpmn:manualTask>

            <!-- Parallel Join -->
            <bpmn:parallelGateway id="Gateway_Parallel_Join" name="Synchronize">
                <bpmn:incoming>SubFlow_P1_End</bpmn:incoming>
                <bpmn:incoming>SubFlow_P2_End</bpmn:incoming>
                <bpmn:incoming>SubFlow_P3_End</bpmn:incoming>
                <bpmn:outgoing>SubFlow_AfterParallel</bpmn:outgoing>
            </bpmn:parallelGateway>

            <!-- Nested Subprocess (Level 2) -->
            <bpmn:subProcess id="SubProcess_Nested" name="Nested Calculation Engine">
                <bpmn:incoming>SubFlow_AfterParallel</bpmn:incoming>
                <bpmn:outgoing>SubFlow_AfterNested</bpmn:outgoing>

                <bpmn:startEvent id="SubStart_Nested" name="Start Calculation"/>

                <bpmn:serviceTask id="Task_Calculate" name="Run Calculation">
                    <bpmn:multiInstanceLoopCharacteristics isSequential="false"/>
                </bpmn:serviceTask>

                <bpmn:endEvent id="SubEnd_Nested" name="Calculation Complete"/>

                <bpmn:sequenceFlow id="NestedFlow_1" sourceRef="SubStart_Nested" targetRef="Task_Calculate"/>
                <bpmn:sequenceFlow id="NestedFlow_2" sourceRef="Task_Calculate" targetRef="SubEnd_Nested"/>
            </bpmn:subProcess>

            <!-- Subprocess End -->
            <bpmn:endEvent id="SubEnd_Main" name="Processing Complete"/>

            <!-- Subprocess Internal Flows -->
            <bpmn:sequenceFlow id="SubFlow_1" sourceRef="SubStart_Main" targetRef="Gateway_Parallel_Split"/>
            <bpmn:sequenceFlow id="SubFlow_P1" sourceRef="Gateway_Parallel_Split" targetRef="Task_Script_Transform"/>
            <bpmn:sequenceFlow id="SubFlow_P2" sourceRef="Gateway_Parallel_Split" targetRef="Task_BusinessRule"/>
            <bpmn:sequenceFlow id="SubFlow_P3" sourceRef="Gateway_Parallel_Split" targetRef="Task_ManualReview"/>
            <bpmn:sequenceFlow id="SubFlow_P1_End" sourceRef="Task_Script_Transform" targetRef="Gateway_Parallel_Join"/>
            <bpmn:sequenceFlow id="SubFlow_P2_End" sourceRef="Task_BusinessRule" targetRef="Gateway_Parallel_Join"/>
            <bpmn:sequenceFlow id="SubFlow_P3_End" sourceRef="Task_ManualReview" targetRef="Gateway_Parallel_Join"/>
            <bpmn:sequenceFlow id="SubFlow_AfterParallel" sourceRef="Gateway_Parallel_Join" targetRef="SubProcess_Nested"/>
            <bpmn:sequenceFlow id="SubFlow_AfterNested" sourceRef="SubProcess_Nested" targetRef="SubEnd_Main"/>
        </bpmn:subProcess>

        <!-- Boundary Events on Subprocess -->
        <bpmn:boundaryEvent id="Boundary_Timer" name="Timeout" attachedToRef="SubProcess_MainProcessing" cancelActivity="true">
            <bpmn:outgoing>Flow_TimeoutPath</bpmn:outgoing>
            <bpmn:timerEventDefinition id="TimerDef_Boundary">
                <bpmn:timeDuration>PT30M</bpmn:timeDuration>
            </bpmn:timerEventDefinition>
        </bpmn:boundaryEvent>

        <bpmn:boundaryEvent id="Boundary_Error" name="Error Caught" attachedToRef="SubProcess_MainProcessing" cancelActivity="true">
            <bpmn:outgoing>Flow_ErrorPath</bpmn:outgoing>
            <bpmn:errorEventDefinition id="ErrDef_Boundary" errorRef="Error_System"/>
        </bpmn:boundaryEvent>

        <bpmn:boundaryEvent id="Boundary_Message_NonInterrupt" name="Update Received" attachedToRef="SubProcess_MainProcessing" cancelActivity="false">
            <bpmn:outgoing>Flow_UpdatePath</bpmn:outgoing>
            <bpmn:messageEventDefinition id="MsgDef_Boundary" messageRef="Message_Notification"/>
        </bpmn:boundaryEvent>

        <!-- ============== PROCESSING OUTCOME ============== -->

        <!-- Event-Based Gateway -->
        <bpmn:eventBasedGateway id="Gateway_ProcessingOutcome" name="Wait for Outcome">
            <bpmn:incoming>Flow_ProcessingComplete</bpmn:incoming>
            <bpmn:outgoing>Flow_WaitAPI</bpmn:outgoing>
            <bpmn:outgoing>Flow_WaitTimer</bpmn:outgoing>
            <bpmn:outgoing>Flow_WaitSignal</bpmn:outgoing>
        </bpmn:eventBasedGateway>

        <!-- Intermediate Catch Events for Event-Based Gateway -->
        <bpmn:intermediateCatchEvent id="Event_CatchAPIResponse" name="API Response">
            <bpmn:incoming>Flow_WaitAPI</bpmn:incoming>
            <bpmn:outgoing>Flow_APIReceived</bpmn:outgoing>
            <bpmn:messageEventDefinition id="MsgDef_Catch" messageRef="Message_Response"/>
        </bpmn:intermediateCatchEvent>

        <bpmn:intermediateCatchEvent id="Event_CatchTimeout" name="Timeout Elapsed">
            <bpmn:incoming>Flow_WaitTimer</bpmn:incoming>
            <bpmn:outgoing>Flow_TimeoutElapsed</bpmn:outgoing>
            <bpmn:timerEventDefinition id="TimerDef_Catch">
                <bpmn:timeDuration>PT5M</bpmn:timeDuration>
            </bpmn:timerEventDefinition>
        </bpmn:intermediateCatchEvent>

        <bpmn:intermediateCatchEvent id="Event_CatchSignal" name="Cancel Signal">
            <bpmn:incoming>Flow_WaitSignal</bpmn:incoming>
            <bpmn:outgoing>Flow_SignalReceived</bpmn:outgoing>
            <bpmn:signalEventDefinition id="SignalDef_Catch" signalRef="Signal_Cancel"/>
        </bpmn:intermediateCatchEvent>

        <!-- ============== BUSINESS RULES ============== -->

        <bpmn:businessRuleTask id="Task_ApplyBusinessRules" name="Apply Business Rules Engine">
            <bpmn:incoming>Flow_APIReceived</bpmn:incoming>
            <bpmn:incoming>Flow_TimeoutElapsed</bpmn:incoming>
            <bpmn:outgoing>Flow_RulesApplied</bpmn:outgoing>
        </bpmn:businessRuleTask>

        <!-- Complex Gateway -->
        <bpmn:complexGateway id="Gateway_RulesDecision" name="Complex Decision" default="Flow_RulesDefault">
            <bpmn:incoming>Flow_RulesApplied</bpmn:incoming>
            <bpmn:outgoing>Flow_RulesApprove</bpmn:outgoing>
            <bpmn:outgoing>Flow_RulesReview</bpmn:outgoing>
            <bpmn:outgoing>Flow_RulesReject</bpmn:outgoing>
            <bpmn:outgoing>Flow_RulesDefault</bpmn:outgoing>
        </bpmn:complexGateway>

        <bpmn:serviceTask id="Task_HandleException" name="Handle Exception Case">
            <bpmn:incoming>Flow_RulesReject</bpmn:incoming>
            <bpmn:incoming>Flow_SignalReceived</bpmn:incoming>
            <bpmn:outgoing>Flow_ExceptionHandled</bpmn:outgoing>
        </bpmn:serviceTask>

        <!-- ============== RESPONSE HANDLING ============== -->

        <bpmn:serviceTask id="Task_PrepareResponse" name="Prepare Response">
            <bpmn:incoming>Flow_RulesApprove</bpmn:incoming>
            <bpmn:incoming>Flow_RulesReview</bpmn:incoming>
            <bpmn:incoming>Flow_RulesDefault</bpmn:incoming>
            <bpmn:outgoing>Flow_ResponseReady</bpmn:outgoing>
        </bpmn:serviceTask>

        <bpmn:sendTask id="Task_SendResponse" name="Send Response to Customer">
            <bpmn:incoming>Flow_ResponseReady</bpmn:incoming>
            <bpmn:outgoing>Flow_ResponseSent</bpmn:outgoing>
        </bpmn:sendTask>

        <!-- ============== INTEGRATION TASKS ============== -->

        <bpmn:serviceTask id="Task_CallExternalAPI" name="Call External API">
            <bpmn:incoming>Flow_UpdatePath</bpmn:incoming>
            <bpmn:outgoing>Flow_APICallComplete</bpmn:outgoing>
        </bpmn:serviceTask>

        <bpmn:serviceTask id="Task_SyncData" name="Synchronize with Legacy">
            <bpmn:incoming>Flow_ResponseSent</bpmn:incoming>
            <bpmn:outgoing>Flow_Synced</bpmn:outgoing>
            <bpmn:standardLoopCharacteristics/>
        </bpmn:serviceTask>

        <bpmn:serviceTask id="Task_LogAudit" name="Log to Audit System">
            <bpmn:incoming>Flow_Synced</bpmn:incoming>
            <bpmn:outgoing>Flow_Logged</bpmn:outgoing>
        </bpmn:serviceTask>

        <!-- ============== END EVENTS ============== -->

        <!-- Success End -->
        <bpmn:endEvent id="End_Success" name="Completed Successfully">
            <bpmn:incoming>Flow_Logged</bpmn:incoming>
        </bpmn:endEvent>

        <!-- Error End -->
        <bpmn:endEvent id="End_Error" name="Error Occurred">
            <bpmn:incoming>Flow_TimeoutPath</bpmn:incoming>
            <bpmn:incoming>Flow_ErrorPath</bpmn:incoming>
            <bpmn:incoming>Flow_ExceptionHandled</bpmn:incoming>
            <bpmn:errorEventDefinition id="ErrDef_End" errorRef="Error_System"/>
        </bpmn:endEvent>

        <!-- Terminate End -->
        <bpmn:endEvent id="End_Terminate" name="Process Terminated">
            <bpmn:terminateEventDefinition id="TermDef_End"/>
        </bpmn:endEvent>

        <!-- ============== DATA OBJECTS ============== -->

        <bpmn:dataObject id="DataObject_Request" name="Request Data">
            <bpmn:dataState id="DS_Request" name="Validated"/>
        </bpmn:dataObject>

        <bpmn:dataObject id="DataObject_Response" name="Response Payload"/>

        <bpmn:dataObjectReference id="DataObjectRef_Config" name="Configuration Settings" dataObjectRef="DataObject_Request"/>

        <bpmn:dataStoreReference id="DataStoreRef_MainDB" name="Main Database" dataStoreRef="DataStore_MainDB"/>

        <!-- ============== TEXT ANNOTATIONS ============== -->

        <bpmn:textAnnotation id="Annotation_1">
            <bpmn:text>This is a very long annotation text that should test text wrapping capabilities and label rendering in the Draw.io output. It contains multiple sentences to ensure the converter handles long text properly.</bpmn:text>
        </bpmn:textAnnotation>

        <bpmn:textAnnotation id="Annotation_2">
            <bpmn:text>XY</bpmn:text>
        </bpmn:textAnnotation>

        <bpmn:association id="Assoc_1" sourceRef="Task_ValidateInput" targetRef="Annotation_1"/>
        <bpmn:association id="Assoc_2" sourceRef="Gateway_RulesDecision" targetRef="Annotation_2"/>

        <!-- ============== SEQUENCE FLOWS ============== -->

        <bpmn:sequenceFlow id="Flow_1" sourceRef="Start_Message_Main" targetRef="Task_ReceiveRequest"/>
        <bpmn:sequenceFlow id="Flow_TimerToReceive" sourceRef="Start_Timer_Scheduled" targetRef="Task_ReceiveRequest"/>
        <bpmn:sequenceFlow id="Flow_SignalToReceive" sourceRef="Start_Signal_Alert" targetRef="Task_ReceiveRequest"/>
        <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_ReceiveRequest" targetRef="Gateway_RequestType"/>

        <!-- Conditional flows from XOR Gateway -->
        <bpmn:sequenceFlow id="Flow_TypeA" name="Type A" sourceRef="Gateway_RequestType" targetRef="Task_ValidateInput">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${type == 'A'}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_TypeB" name="Type B" sourceRef="Gateway_RequestType" targetRef="Task_ValidateInput">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${type == 'B'}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_TypeC" name="Type C" sourceRef="Gateway_RequestType" targetRef="Task_ValidateInput">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${type == 'C'}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_Default" name="Other (Default)" sourceRef="Gateway_RequestType" targetRef="Task_ValidateInput"/>

        <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_ValidateInput" targetRef="Gateway_ValidationResult"/>

        <bpmn:sequenceFlow id="Flow_Valid" name="Valid" sourceRef="Gateway_ValidationResult" targetRef="SubProcess_MainProcessing">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${valid == true}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_PartialValid" name="Partial" sourceRef="Gateway_ValidationResult" targetRef="SubProcess_MainProcessing">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${partial == true}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_Invalid" name="Invalid" sourceRef="Gateway_ValidationResult" targetRef="Task_RejectInvalid">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${valid == false}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_ToRejected" sourceRef="Task_RejectInvalid" targetRef="End_Rejected"/>

        <!-- Boundary event flows -->
        <bpmn:sequenceFlow id="Flow_TimeoutPath" sourceRef="Boundary_Timer" targetRef="End_Error"/>
        <bpmn:sequenceFlow id="Flow_ErrorPath" sourceRef="Boundary_Error" targetRef="End_Error"/>
        <bpmn:sequenceFlow id="Flow_UpdatePath" sourceRef="Boundary_Message_NonInterrupt" targetRef="Task_CallExternalAPI"/>

        <bpmn:sequenceFlow id="Flow_ProcessingComplete" sourceRef="SubProcess_MainProcessing" targetRef="Gateway_ProcessingOutcome"/>

