        <bpmn:sequenceFlow id="Flow_Synced" sourceRef="Task_SyncData" targetRef="Task_LogAudit"/>
        <bpmn:sequenceFlow id="Flow_Logged" sourceRef="Task_LogAudit" targetRef="End_Success"/>
        <bpmn:sequenceFlow id="Flow_APICallComplete" sourceRef="Task_CallExternalAPI" targetRef="End_Terminate"/>

