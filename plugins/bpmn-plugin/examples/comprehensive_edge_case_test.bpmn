        <bpmn:sequenceFlow id="Flow_WaitTimer" sourceRef="Gateway_ProcessingOutcome" targetRef="Event_CatchTimeout"/>
        <bpmn:sequenceFlow id="Flow_WaitSignal" sourceRef="Gateway_ProcessingOutcome" targetRef="Event_CatchSignal"/>

        <bpmn:sequenceFlow id="Flow_APIReceived" sourceRef="Event_CatchAPIResponse" targetRef="Task_ApplyBusinessRules"/>
        <bpmn:sequenceFlow id="Flow_TimeoutElapsed" sourceRef="Event_CatchTimeout" targetRef="Task_ApplyBusinessRules"/>
        <bpmn:sequenceFlow id="Flow_SignalReceived" sourceRef="Event_CatchSignal" targetRef="Task_HandleException"/>

        <bpmn:sequenceFlow id="Flow_RulesApplied" sourceRef="Task_ApplyBusinessRules" targetRef="Gateway_RulesDecision"/>

        <bpmn:sequenceFlow id="Flow_RulesApprove" name="Approve" sourceRef="Gateway_RulesDecision" targetRef="Task_PrepareResponse">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${decision == 'approve'}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_RulesReview" name="Review" sourceRef="Gateway_RulesDecision" targetRef="Task_PrepareResponse">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${decision == 'review'}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_RulesReject" name="Reject" sourceRef="Gateway_RulesDecision" targetRef="Task_HandleException">
            <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${decision == 'reject'}</bpmn:conditionExpression>
        </bpmn:sequenceFlow>
        <bpmn:sequenceFlow id="Flow_RulesDefault" name="Default" sourceRef="Gateway_RulesDecision" targetRef="Task_PrepareResponse"/>

        <bpmn:sequenceFlow id="Flow_ExceptionHandled" sourceRef="Task_HandleException" targetRef="End_Error"/>

